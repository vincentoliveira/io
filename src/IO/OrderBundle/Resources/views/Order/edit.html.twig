{% extends "IOCarteBundle::layout.html.twig" %}

{% block content %}
    <h3>
        Modifier la commande #{{ entity.id}}
    </h3>

    {{ form_start(edit_form) }}
        <div class="col-md-6">
            <div class="form-group">
                {{ form_row(edit_form.tableName) }}
            </div>
            <div class="form-group">
                {{ form_row(edit_form.status) }}
            </div>
        </div>
        <div class="clearfix"></div>
        
        {{ form_rest(edit_form) }}
    {{ form_end(edit_form) }}
{% endblock %}
