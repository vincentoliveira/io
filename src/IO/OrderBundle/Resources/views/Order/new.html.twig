{% extends "IOCarteBundle::layout.html.twig" %}

{% block content %}
    <h3>
        Ajouter une commande
    </h3>

    {{ form_start(form) }}
        <div class="col-md-6">
            <div class="form-group">
                {{ form_row(form.tableName) }}
            </div>
            <div class="form-group">
                {{ form_row(form.status) }}
            </div>
        </div>
        <div class="clearfix"></div>
        
        {{ form_rest(form) }}
    {{ form_end(form) }}
{% endblock %}
